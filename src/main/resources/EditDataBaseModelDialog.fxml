<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" spacing="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane fx:id="grid" vgap="5"/>

    <HBox alignment="CENTER" spacing="10">
        <Button fx:id="okBtn" prefHeight="26.0" prefWidth="89.0" text="OK" />
        <Button fx:id="cancelBtn" prefHeight="26.0" prefWidth="100.0" text="Cancel" />

        <VBox.margin>
         <Insets bottom="10.0" />
        </VBox.margin>
    </HBox>

   <padding>
      <Insets left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
